{
  "name": "plemory",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "setup": "rm -rf node_modules yarn.lock package-lock.json ios android .expo && yarn install && expo prebuild --clean -p ios --no-install && cd ios && pod install && cd .. && expo prebuild --clean -p android --no-install",
    "setup:android": "yarn cache clean && rm -rf node_modules yarn.lock package-lock.json ios android .expo && yarn install && expo prebuild --clean -p android --no-install"
  },
  "dependencies": {
    "@react-native-community/slider": "^4.2.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "@types/react": "^17.0.43",
    "@viro-community/react-viro": "^2.23.0",
    "expo": "~44.0.0",
    "expo-modules-core": "^0.6.5",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-maps": "^0.31.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-web": "0.17.1",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/react": "~17.0.21",
    "@types/react-dom": "^18.0.0",
    "@types/react-native": "~0.64.12",
    "typescript": "~4.3.5"
  },
  "private": true
}
